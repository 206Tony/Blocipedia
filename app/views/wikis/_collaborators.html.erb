<h4>Collaborators</h4>
  <table class="table">
    <% @wiki.collaborators.each do |collaborator| %>
      <tr>
        <td><%= collaborator.user.email %></td>
        <td><%= link_to 'Remove Collaborator', wiki_collaborators_path(collaborator.id), method: :delete, class: 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </table>
  <%= link_to "Add Collaborator", wiki_collaborators_path(wiki_id: wiki), class: 'btn btn-success' %> 
