<h1>Wikis</h1>
  <div class="row">
    <div class="col-md-5">
      <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
    </div>
    <br>
    <div class="col-md-5">
      <% @wikis.each do |wiki| %>
        <div class="media">
          <div class="media-body">
            <div class="media-heading">
              <%= link_to (wiki.title), wiki %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>